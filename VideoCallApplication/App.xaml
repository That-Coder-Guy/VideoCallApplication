<Application x:Class="VideoCallApplication.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VideoCallApplication"
             StartupUri="MainWindow.xaml"
    xmlns:System="clr-namespace:System;assembly=System.Runtime">
    <Application.Resources>
        <FontFamily x:Key="PrimaryFont">Bahnschrift SemiBold</FontFamily>
        <System:Double x:Key="ButtonFontSize">14</System:Double>
        <System:Double x:Key="HeaderFontSize">24</System:Double>
        <FontFamily x:Key="TextBlockFont">Bahnschrift SemiLight</FontFamily>
        <System:Double x:Key="TextBlockFontSize">12</System:Double>
    </Application.Resources>
</Application>
